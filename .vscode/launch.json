{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch in Chrome",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceRoot}/src",
      "breakOnLoad": true,
      "sourceMaps": true
    },
    {
      "name": "Launch in Edge",
      "type": "pwa-msedge",
      "request": "launch",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceRoot}/src",
      "sourceMaps": true
    },
    {
      "name": "Launch in FireFox",
      "type": "firefox",
      "request": "launch",
      "url": "http://localhost:3000"
    }
  ]
}
